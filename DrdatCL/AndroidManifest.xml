<permission android:description="get a drdat task list " android:protectionLevel="normal" android:permissionGroup="com.google.android.drdat.DRDAT" android:name="com.google.android.drdat.DRDAT_TASKLIST"></permission>
<uses-permission android:name="com.google.android.drdat.DRDAT_TASKLIST"></uses-permission>
<permission android:description="get a drdat task" android:protectionLevel="normal" android:permissionGroup="com.google.android.drdat.DRDAT" android:name="com.google.android.drdat.DRDAT_TASK"></permission>
<uses-permission android:name="com.google.android.drdat.DRDAT_TASK"></uses-permission>
<uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest> 